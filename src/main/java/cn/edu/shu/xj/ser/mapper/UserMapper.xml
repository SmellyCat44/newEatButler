<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.shu.xj.ser.mapper.UserMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="UserResultMap" type="cn.edu.shu.xj.ser.entity.User">
        <result column="user_id" property="userId"/>
        <result column="user_pwd" property="userPwd"/>
        <result column="user_name" property="userName"/>
        <result column="sex" property="sex"/>
        <result column="user_tel" property="userTel"/>
        <result column="email" property="email"/>
        <result column="user_city" property="userCity"/>
<!--        <result column="reg_time" property="regTime"/>-->
        <result column="is_vip" property="isVip"/>
        <result column="vip_due_date" property="vipDueDate"/>
        <result column="user_balance" property="userBalance"/>
    </resultMap>
</mapper>
